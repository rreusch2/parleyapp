# WebSocket Service Configuration
PORT=8080
NODE_ENV=development

# Daytona Configuration
DAYTONA_API_KEY=your_daytona_api_key_here
DAYTONA_WORKSPACE_URL=https://your-workspace.daytona.io
DAYTONA_AGENT_ENDPOINT=/api/v1/agent

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# OpenManus Agent Configuration
OPENMANUS_AGENT_URL=http://localhost:3000
OPENMANUS_API_KEY=your_openmanus_api_key

# Security
JWT_SECRET=your_jwt_secret_for_websocket_auth
CORS_ORIGIN=http://localhost:3001

# Logging
LOG_LEVEL=debug

# Tool Configuration
ENABLE_BROWSER_TOOL=true
ENABLE_STATMUSE_TOOL=true
ENABLE_WEB_SEARCH_TOOL=true
ENABLE_PYTHON_TOOL=true

# Rate Limiting
MAX_CONNECTIONS_PER_USER=3
MESSAGE_RATE_LIMIT=10
RATE_LIMIT_WINDOW=60000
