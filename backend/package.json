  "scripts": {
    "dev": "cross-env EXPO_NO_TELEMETRY=1 expo start",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "validate-production": "node scripts/validate-production-build.js",
    "check-localhost": "grep -r \"localhost\" app/ --exclude-dir=node_modules || echo 'No localhost references found'",
    "build:production": "NODE_ENV=production expo export",
    "build:ios-production": "NODE_ENV=production expo build:ios",
    "build:android-production": "NODE_ENV=production expo build:android",
    "build:ios": "expo build:ios",
    "build:android": "expo build:android",
    "pre-build-check": "npm run validate-production && npm run check-localhost",
    "app-store-ready": "npm run pre-build-check && echo 'âœ… App Store preparation complete!'"
