{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for AI-powered sports betting app",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ai-tools": "ts-node src/scripts/testAiTools.ts",
    "check-duplicates": "ts-node src/scripts/checkDuplicates.ts",
    "check-game-statuses": "ts-node src/scripts/checkGameStatuses.ts",
    "fetch-upcoming-mlb": "ts-node src/scripts/fetchUpcomingMLB.ts",
    "fetch-today-mlb": "ts-node src/scripts/fetchTodayMLB.ts",
    "fetch-today-nba": "ts-node src/scripts/fetchTodayNBA.ts",
    "fetch-tomorrow-games": "ts-node src/scripts/fetchTomorrowGames.ts",
    "daily-setup": "npm run fetch-tomorrow-games",
    "init-daily-games": "ts-node src/scripts/dailyGamesFetch.ts",
    "test-enhanced-orchestrator": "ts-node src/scripts/testEnhancedOrchestrator.ts",
    "clear-mlb-games": "ts-node src/scripts/clearMLBGames.ts",
    "cleanup-duplicates": "ts-node src/scripts/cleanupDuplicates.ts",
    "cleanup-duplicates:execute": "ts-node src/scripts/cleanupDuplicates.ts --execute",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.1.0",
    "@react-native-community/datetimepicker": "^8.4.1",
    "@react-native-picker/picker": "^2.11.0",
    "@supabase/supabase-js": "^2.49.9",
    "@types/express-serve-static-core": "^5.0.6",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.x",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "node-fetch": "^2.6.1",
    "openai": "^5.1.1",
    "pg": "^8.x",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.x",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.x",
    "@types/node-cron": "^3.0.10",
    "@types/node-fetch": "^2.6.12",
    "@types/pg": "^8.x",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.x",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.x"
  }
}
